{
  "name": "fp.js",
  "version": "1.0.0",
  "description": "A tiny functional programming library",
  "main": "fp.js",
  "scripts": {
    "lint": "standard",
    "test": "mocha --compilers js:babel-register ./src/**/*_test.js ./src",
    "watch": "mocha --compilers js:babel-register -w ./src/**/*_test.js ./src"
  },
  "author": "Alice Pote",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "sinon": "^1.17.6",
    "standard": "^8.3.0",
    "webpack": "^1.13.2"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-object-assign",
      "transform-object-rest-spread"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  }
}
